{
    "singleQuote": true,
    "quoteProps": "as-needed",
    "trailingComma": "all",
    "printWidth": 120,
    "pluginSearchDirs": ["."],
    "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always",
    "endOfLine": "lf",
    "singleAttributePerLine": false,
    "semi": false
}
